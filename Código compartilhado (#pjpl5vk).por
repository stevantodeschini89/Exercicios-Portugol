programa {
  funcao inicio() {
    cadeia nome_Funcionario 
    real salario
    escreva("Ola, digite o seu nome: ")
    leia(nome_Funcionario)
    escreva("Olá, digite o seu salario: R$")
    leia(salario)
    escreva("O funcionari@ ", nome_Funcionario," Recebeu em junho o salario de R$", salario," REAIS.")
  }
}
