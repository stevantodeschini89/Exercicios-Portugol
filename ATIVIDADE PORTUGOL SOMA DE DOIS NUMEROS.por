programa {
  funcao inicio() {
    real valor_1, valor_2,soma
    escreva("Digite o priemiro valor para uma soma simples de dois numeros inteiros ou reias: ")
    leia(valor_1)
    escreva("Digite o segundo valor para soma: ")
    leia(valor_2)
    soma = valor_1  + valor_2
    escreva("A soma dos dois valores corresponde á: ",soma)
  }
}
